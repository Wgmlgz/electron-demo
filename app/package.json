{
  "name": "electron-demo",
  "version": "0.0.1",
  "private": true,
  "main": "src/electron.cjs",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "package": "npm run build && electron-builder --config electron-builder.config.json",
    "dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir",
    "electron": "concurrently --kill-others \"vite dev\" \"electron src/electron.cjs\"",
    "deploy": "echo '' > build/.nojekyll && gh-pages -d build -t true"
  },
  "devDependencies": {
    "@panzoom/panzoom": "^4.5.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@unocss/extractor-svelte": "^0.52.0",
    "@unocss/transformer-directives": "^0.52.1",
    "axios": "^1.4.0",
    "carbon-components-svelte": "^0.74.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "concurrently": "^8.0.1",
    "electron": "^24.3.1",
    "electron-builder": "^23.6.0",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "moment": "^2.29.4",
    "panzoom": "^9.4.3",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.62.1",
    "svelte": "^3.54.0",
    "svelte-chartjs": "^3.1.2",
    "svelte-check": "^3.0.1",
    "svelte-infinite-viewer": "^0.24.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unocss": "^0.52.0",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "gh-pages": "^5.0.0"
  },
  "type": "module"
}
